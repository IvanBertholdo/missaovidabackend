{
  "version": 2,
  "builds": [
    {
      "src": "app.js", 
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "8mb", 
        "runtime": "nodejs18.x" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "/routes/authRoutes.js" 
    }
  ]
}
