{
  "version": 2,
  "builds": [
    {
      "src": "app.js", 
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "8mb", 
        "runtime": "nodejs18.x" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "/api/healthcheck" 
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/$1" 
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/$2" 
    }
  ]
}
